https://practice.geeksforgeeks.org/problems/palindrome-string0817/1

Time:O(N), Space:O(1)

class Solution{
public:	
	
	
	int isPalindrome(string S)
	{
	    // Your code goes here
	    int n=S.length();
	    int i=0,j=n-1;
	    
	    while(i<j)
	    {
	        if(S[i]!=S[j])
	        {
	            return 0;
	        }
	        i++;j--;
	    }
	    
	    return 1;
	}

};
